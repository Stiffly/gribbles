<?xml version="1.0" encoding="UTF-8"?>

<cml>

<Button id="image-button" x="50" y="200" hit="button-hit-box" initial="image-up" down="image-down" up="image-up"
	dispatch="initial:initial-state:down:down-state:up:up-state:over:over state:out:out-state" >
	<Graphic id="button-hit-box" />
	<Image id="image-up" src="images/buttons/button-image-up.png" />
	<Image id="image-down" src="images/buttons/button-image-down.png" />
</Button>

<Button id="video-button" x="50" y="50" hit="button-hit-box" initial="video-up" down="video-down" up="video-up"
