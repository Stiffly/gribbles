	<Graphic id="button-hit-box"/>		
	<Image id="web-up" src="images/buttons/button-web-up.png" />
	<Image id="web-down" src="images/buttons/button-web-down.png" />
</Button>

<Button id="pdf-button" x="50" y="500" hit="button-hit-box" initial="pdf-up" down="pdf-down" up="pdf-up"
